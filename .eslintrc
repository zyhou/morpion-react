{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "env": {
        "es6": true
    },
    "plugins": ["prettier"],
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings",
        "prettier",
        "plugin:prettier/recommended"
    ],
    "rules": { "no-console": ["error", { "allow": ["warn", "error"] }] }
}
